  "CameraConfig": "D:/RemoteDebug/data/configs/CameraConfig.json",
  "AxisConfig": "D:/RemoteDebug/data/configs/AxisConfig.json",
  "PlcConfig": "D:/RemoteDebug/data/configs/PlcConfig.json",
  "LightController": "D:/RemoteDebug/data/configs/LightController.json",
  "Recipe": "D:/RemoteDebug/data/recipes",
  "InspectionResultDatabase": "D:/RemoteDebug/data/inspection.db",
  "InfererenceModelPath": "D:/RemoteDebug/data/Models/green_cross_03.onnx",
  "SystemConfig": "D:/RemoteDebug/data/configs/systemConfig.json",
  "HansLaserInstallationDir": "D:\\HANS LASER\\Han's Laser Marking SoftwareV6_0_4",
  "configBaseDir": "../../data",
  "shapeLayerPath": "ShapeLayers.json",
  "grpcAddress": "http://localhost:50051",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Debug",
      "Serilog.Sinks.File",
      "Serilog.Expressions"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}][{ThreadId},{ThreadName}] [{Level:u3}][{SourceContext}] {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}][{ThreadId},{ThreadName}] [{Level:u3}][{SourceContext}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}